,t,n,r,o,i,a){var s=getProjectingAngle(t,n),l=t.v[n%t._length],c=t.v[0===n?t._length-1:n-1],u=t.v[(n+1)%t._length],p=2===i?Math.sqrt(Math.pow(l[0]-c[0],2)+Math.pow(l[1]-c[1],2)):0,h=2===i?Math.sqrt(Math.pow(l[0]-u[0],2)+Math.pow(l[1]-u[1],2)):0;setPoint(e,t.v[n%t._length],s,a,r,h/(2*(o+1)),p/(2*(o+1)),i)}function zigZagSegment(e,t,n,r,o,i){for(var a=0;a<r;a+=1){var s=(a+1)/(r+1),l=2===o?Math.sqrt(Math.pow(t.points[3][0]-t.points[0][0],2)+Math.pow(t.points[3][1]-t.points[0][1],2)):0,c=t.normalAngle(s);setPoint(e,t.point(s),c,i,n,l/(2*(r+1)),l/(2*(r+1)),o),i=-i}return i}function linearOffset(e,t,n){var r=Math.atan2(t[0]-e[0],t[1]-e[1]);return[polarOffset(e,r,n),polarOffset(t,r,n)]}function offsetSegment(e,t){var n,r,o,i,a,s,l;n=(l=linearOffset(e.points[0],e.points[1],t))[0],r=l[1],o=(l=linearOffset(e.points[1],e.points[2],t))[0],i=l[1],a=(l=linearOffset(e.points[2],e.points[3],t))[0],s=l[1];var c=lineIntersection(n,r,o,i);null===c&&(c=r);var u=lineIntersection(a,s,o,i);return null===u&&(u=a),new PolynomialBezier(n,c,u,s)}function joinLines(e,t,n,r,o){var i=t.points[3],a=n.points[0];if(3===r)return i;if(pointEqual(i,a))return i;if(2===r){var s=-t.tangentAngle(1),l=-n.tangentAngle(0)+Math.PI,c=lineIntersection(i,polarOffset(i,s+Math.PI/2,100),a,polarOffset(a,s+Math.PI/2,100)),u=c?pointDistance(c,i):pointDistance(i,a)/2,p=polarOffset(i,s,2*u*roundCorner);return e.setXYAt(p[0],p[1],"o",e.length()-1),p=polarOffset(a,l,2*u*roundCorner),e.setTripleAt(a[0],a[1],a[0],a[1],p[0],p[1],e.length()),a}var h=lineIntersection(pointEqual(i,t.points[2])?t.points[0]:t.points[2],i,a,pointEqual(a,n.points[1])?n.points[3]:n.points[1]);return h&&pointDistance(h,i)<o?(e.setTripleAt(h[0],h[1],h[0],h[1],h[0],h[1],e.length()),h):i}function getIntersection(e,t){var n=e.intersections(t);return n.length&&floatEqual(n[0][0],1)&&n.shift(),n.length?n[0]:null}function pruneSegmentIntersection(e,t){var n=e.slice(),r=t.slice(),o=getIntersection(e[e.length-1],t[0]);return o&&(n[e.length-1]=e[e.length-1].s